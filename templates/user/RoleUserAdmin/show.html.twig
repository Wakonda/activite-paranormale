{% extends "admin/GenericAdmin/show.html.twig" %}

{% block h2_title %}{{ "admin.index.UserRole"|trans([], "validators") }}{% endblock %}

{% block edit_admin_route %}{{ path('RoleUser_Admin_Edit', { 'id': entity.id }) }}{% endblock %}
{% block new_admin_route %}{{ path('RoleUser_Admin_New') }}{% endblock %}
{% block index_admin_route %}{{ path('RoleUser_Admin_Index') }}{% endblock %}
{% block delete_admin_route %}{{ path('RoleUser_Admin_Delete', { 'id': entity.id }) }}{% endblock %}
{% block restore_admin_route%}{{ path('RoleUser_Admin_Restore', { 'id': entity.id }) }}{% endblock %}

{% block tbody_admin_table %}
